<% @page_title = "ユーザー編集" %>
<h1><%= @page_title %></h1>

<p><%= link_to "ユーザー情報一覧", @users %></p>

<% @user = User.edit unless @user %>
<%= form_for @user do |form| %>

<br>ユーザー名<%= form.text_field :name ,size: 16 %></br>

<br>パスワード<%= form.password_field :password ,size: 12 %></br>

<p><%= form.submit %></p>
<% end %>
